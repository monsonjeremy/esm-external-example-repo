{
  "name": "@monsonjeremy/esm-external-package",
  "version": "0.0.1",
  "description": "Example package",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:esm": "tsc",
    "build:cjs": "tsc --outDir lib/cjs --module commonjs",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --no-cache --runInBand --passWithNoTests",
    "test:coverage": "jest --coverage --no-cache --runInBand",
    "package": "npm run build && npm pack"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@solana/wallet-adapter-wallets": "^0.15.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  }
}
